{% if actions|length > 0 %}
<ul>
{% for action in actions %}
    <li>{% include "merkNotificationBundle:Action:summary.html.twig" with {"action": action} %}</li>
{% endfor %}
</ul>
{% else %}
<p><strong>No notifications</strong></p>
{% endif %}
